<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.twentyfortyeight.GameProcess">
   <children>
      <Pane fx:id="pane" maxHeight="450.0" maxWidth="450.0" minHeight="450.0" minWidth="450.0" onMousePressed="#mouseUsed" onMouseReleased="#mouseUsed" prefHeight="450.0" prefWidth="450.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin></Pane>
      <Button fx:id="mainMenuButton" mnemonicParsing="false" onAction="#backToMenu" text="Меню">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <font>
            <Font name="Noto Sans Regular" size="13.0" />
         </font></Button>
   </children>
</VBox>
